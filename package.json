{
  "name": "ai-data-app",
  "version": "1.0.0",
  "description": "AI App that changes your data to usefull information",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "test",
    "make": "npx electron-builder"
  },
  "author": "Jason",
  "license": "ISC",
  "devDependencies": {
    "electron": "^35.0.2",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "jszip": "^3.10.1"
  },
  "build": {
    "appId": "com.jasonvanhootegem.AIDataApp",
    "productName": "AI Data App",
    "win": {
      "target": "nsis"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "publish": []
  }
}